
{{>layouts/header}}
{{#member}}
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Email</th>
      <th scope="col">Password</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th>{{id}}</th>
      <td>{{email}}</td>
      <td>{{password}}</td>
    </tr>
  </tbody>
</table>
<a href = "/members/{{id}}/edit" class = "btn btn-primary">수정하기</a>
<a href = "/members">Go to Member List</a>
{{/member}}
{{>layouts/footer}}

